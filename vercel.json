{
  "functions": {
    "api/products.js": {
      "runtime": "@vercel/node@3"
    },
    "api/admin/products.js": {
      "runtime": "@vercel/node@3"
    },
    "api/testimonials.js": {
      "runtime": "@vercel/node@3"
    },
    "api/cart.js": {
      "runtime": "@vercel/node@3"
    },
    "api/cart-items.js": {
      "runtime": "@vercel/node@3"
    },
    "api/checkout.js": {
      "runtime": "@vercel/node@3"
    },
    "api/send-download-email.js": {
      "runtime": "@vercel/node@3"
    }
  },
  "rewrites": [
    {
      "source": "/api/products/([^/]+)",
      "destination": "/api/products?id=$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
