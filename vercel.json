{
  "functions": {
    "api/products.js": {
      "runtime": "nodejs18.x"
    },
    "api/admin/products.js": {
      "runtime": "nodejs18.x"
    },
    "api/testimonials.js": {
      "runtime": "nodejs18.x"
    },
    "api/cart.js": {
      "runtime": "nodejs18.x"
    },
    "api/cart-items.js": {
      "runtime": "nodejs18.x"
    },
    "api/checkout.js": {
      "runtime": "nodejs18.x"
    },
    "api/send-download-email.js": {
      "runtime": "nodejs18.x"
    }
  },
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "routes": [
    {
      "src": "/api/products/([^/]+)",
      "dest": "/api/products?id=$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
