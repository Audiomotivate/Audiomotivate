"builds": [
  {
    "src": "api/products/index.js",
    "use": "@vercel/node"
  },
  {
    "src": "api/admin/products/index.js", 
    "use": "@vercel/node"
  }
],
"routes": [
  {
    "src": "/api/products",
    "dest": "/api/products/index.js"
  },
  {
    "src": "/api/admin/products",
    "dest": "/api/admin/products/index.js"
  }
]
