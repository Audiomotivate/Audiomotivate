{
  "functions": {
    "api/products.js": {
      "runtime": "@vercel/node"
    },
    "api/cart.js": {
      "runtime": "@vercel/node"
    },
    "api/testimonials.js": {
      "runtime": "@vercel/node"
    },
    "api/checkout.js": {
      "runtime": "@vercel/node"
    },
    "api/send-download-email.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
